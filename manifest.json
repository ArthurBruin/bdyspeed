{
	"manifest_version": 2,
	"name": "bdyspeed",
    "version": "1.0.0",
    "description": "百度网盘视频加速器",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
            "js": ["content-script.js"],
			"run_at": "document_start"
		}
	],
	"browser_action": {
        "default_title": "百度网盘视频加速器",
        "default_popup": "popup.html"
    },
    "web_accessible_resources": ["content-script.js"]
}